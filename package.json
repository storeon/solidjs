{
  "name": "@storeon/solidjs",
  "version": "2.0.0",
  "description": "A tiny connector for Storeon and Solid.js",
  "main": "index.js",
  "types": "index.d.ts",
  "repository": "git@github.com:storeon/solidjs.git",
  "author": "Dmytro Mostovyi <djmakaron2009@gmail.com>",
  "license": "MIT",
  "private": false,
  "sideEffects": false,
  "engines": {
    "node": ">=10"
  },
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "keywords": [
    "storeon",
    "state",
    "solid-js",
    "solid"
  ],
  "scripts": {
    "start": "parcel serve example/index.html --open",
    "lint": "eslint ./*.js",
    "test": "jest --coverage && yarn lint && size-limit"
  },
  "peerDependencies": {
    "solid-js": "^0.22.0",
    "storeon": "^3.0.0"
  },
  "devDependencies": {
    "@logux/eslint-config": "^42.1.0",
    "@size-limit/dual-publish": "^4.7.0",
    "@size-limit/preset-small-lib": "^4.7.0",
    "dual-publish": "^0.11.0",
    "eslint": "^7.12.1",
    "eslint-config-standard": "^16.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prefer-let": "^1.1.0",
    "eslint-plugin-prettierx": "^0.14.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^4.0.2",
    "eslint-plugin-unicorn": "^23.0.0",
    "jest": "^26.6.1",
    "size-limit": "^4.7.0",
    "solid-js": "^0.21.4",
    "storeon": "^3.1.1"
  },
  "size-limit": [
    {
      "name": "core",
      "import": {
        "index.js": "{ StoreonProvider, useStoreon }"
      },
      "limit": "172 B",
      "ignore": [
        "solid-js"
      ]
    }
  ],
  "eslintConfig": {
    "extends": "@logux/eslint-config"
  }
}
