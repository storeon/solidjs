{
  "name": "@storeon/solidjs",
  "version": "1.0.0",
  "description": "A tiny connector for Storeon and Solid.js",
  "main": "index.js",
  "types": "index.d.ts",
  "repository": "git@github.com:storeon/solidjs.git",
  "author": "Dmytro Mostovyi <djmakaron2009@gmail.com>",
  "license": "MIT",
  "private": false,
  "sideEffects": false,
  "engines": {
    "node": ">=10"
  },
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "keywords": [
    "storeon",
    "state",
    "solid-js",
    "solid"
  ],
  "scripts": {
    "start": "parcel serve example/index.html --open",
    "lint": "eslint ./*.js",
    "test": "jest --coverage && yarn lint && size-limit"
  },
  "peerDependencies": {
    "solid-js": "^0.18.0",
    "storeon": "^3.0.0"
  },
  "devDependencies": {
    "@logux/eslint-config": "^40.0.3",
    "@size-limit/dual-publish": "^4.5.7",
    "@size-limit/preset-small-lib": "^4.5.7",
    "dual-publish": "^0.11.0",
    "eslint": "^7.7.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prefer-let": "^1.0.2",
    "eslint-plugin-prettierx": "^0.14.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-unicorn": "^21.0.0",
    "jest": "^26.4.2",
    "size-limit": "^4.5.7",
    "solid-js": "^0.20.1",
    "storeon": "^3.0.3"
  },
  "size-limit": [
    {
      "name": "core",
      "import": {
        "index.js": "{ StoreonProvider, useStoreon }"
      },
      "limit": "172 B",
      "ignore": [
        "solid-js"
      ]
    }
  ],
  "eslintConfig": {
    "extends": "@logux/eslint-config"
  }
}
